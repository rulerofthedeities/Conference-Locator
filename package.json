{
  "name": "conferences",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && concurrently \"npm run watch:ts\" \"nodemon server.js --ignore public/\" ",
    "build": "concurrently \"npm run build:by\" \"npm run build:ts\"",
    "build:bw": "browserify public/assets/js/browserify_libs.js -o public/assets/js/libs.js",
    "watch:bw": "watchify public/assets/js/browserify_libs.js -o public/assets/js/libs.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "browsersync": "browser-sync start --files \"public/**/*.*\" --proxy 127.127.0.1:8080 --reload-delay=300 --reload-debounce=500",
    "postinstall": "typings install",
    "typings": "typings"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.3",
    "@angular/compiler": "2.0.0-rc.3",
    "@angular/core": "2.0.0-rc.3",
    "@angular/forms": "0.1.1",
    "@angular/http": "2.0.0-rc.3",
    "@angular/platform-browser": "2.0.0-rc.3",
    "@angular/platform-browser-dynamic": "2.0.0-rc.3",
    "@angular/router": "3.0.0-alpha.7",
    "@angular/upgrade": "2.0.0-rc.3",
    "body-parser": "^1.14.0",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.0",
    "express": "^4.13.3",
    "font-awesome": "^4.6.3",
    "mongodb": "^2.0.49",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "concurrently": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "nodemon": "^1.9.2",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
